{
	"name": "router2",
	"version": "1.0.0",
	"description": "",
	"keywords": [],
	"homepage": "",
	"bugs": "",
	"repository": "",
	"author": "Yoriiis aka Joris DANIEL",
	"exports": {
		".": {
			"import": "./dist/tunnel.esm.js",
			"require": "./dist/tunnel.cjs.js",
			"default": "./dist/tunnel.js"
		},
		"./jsx": {
			"import": "./dist/jsx.esm.js",
			"require": "./dist/jsx.cjs.js",
			"default": "./dist/jsx.js"
		}
	},
	"main": "dist/tunnel.cjs.js",
	"files": [
		"dist/*.js"
	],
	"scripts": {
		"build": "rm -rf ./dist/ && rollup --config --environment ENV:production",
		"coverage": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
		"start": "rm -rf ./dist/ && rollup --config --environment ENV:development --watch --sourcemap",
		"test": "jest --verbose --coverage",
		"test:coverage": "jest --coverage",
		"test:eslint": "eslint . --ignore-pattern node_modules --ignore-pattern dist",
		"test:markdown": "markdownlint '**/*.md' --ignore node_modules",
		"test:type-check": "tsc --noEmit",
		"test:watch": "jest --watchAll --coverage",
		"watch:type-check": "tsc --noEmit --watch"
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@rollup/plugin-buble": "^0.21.3",
		"@rollup/plugin-typescript": "^8.3.0",
		"@types/node": "^16.11.9",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-typescript-sort-keys": "^2.0.0",
		"jest": "^27.3.1",
		"markdownlint-cli": "^0.29.0",
		"prettier": "^2.4.1",
		"rollup": "^2.60.0",
		"rollup-plugin-terser": "^7.0.2",
		"tslib": "^2.3.1",
		"typescript": "^4.4.4"
	},
	"engines": {
		"node": ">= 12"
	}
}
